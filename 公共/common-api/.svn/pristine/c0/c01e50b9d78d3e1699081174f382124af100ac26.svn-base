package com.xyscm.framework.common.api;

import java.io.Serializable;

/**
 * 功能描述：
 * 公共JMS消息
 * 
 * @author xielf
 * @CreateDate 2017-8-22 下午4:49:18
 * @Version: 1.0
 */
public class ComJMSMessage implements Serializable{

	private static final long serialVersionUID = -1423868239767533130L;
	
	/** 业务数据类型：机构 */
	public static final String B_TYPE_ORG = "01";
	/** 业务数据类型：部门*/
	public static final String B_TYPE_DEPT = "02";
	/** 业务数据类型：员工 */
	public static final String B_TYPE_EMPLOY = "03";
	/** 业务数据类型：仓库 */
	public static final String B_TYPE_WAREHOUSE = "04";
	/** 业务数据类型：物资代码 */
	public static final String B_TYPE_GOODS_CODE = "05";
	/** 业务数据类型：品名 */
	public static final String B_TYPE_PNAME = "06";
	/** 业务数据类型：产地*/
	public static final String B_TYPE_PAREA = "07";
	/** 业务数据类型：物资调整、物资损溢*/
	public static final String B_TYPE_WAREHOUSR_WLP_LOSS = "08";
	/** 业务数据类型：往来单位*/
	public static final String B_TYPE_COMPANY = "09";
	
	
	/**
	 * 业务类型
	 */
	private String BusinessType;
	
	/**
	 * 业务数据对象
	 */
	private Serializable data;

	/**
	 * @return the businessType
	 */
	public String getBusinessType() {
		return BusinessType;
	}

	/**
	 * @param businessType the businessType to set
	 */
	public void setBusinessType(String businessType) {
		BusinessType = businessType;
	}

	/**
	 * @return the data
	 */
	public Serializable getData() {
		return data;
	}

	/**
	 * @param data the data to set
	 */
	public void setData(Serializable data) {
		this.data = data;
	}
	
	

}
